
<header>

  <li>
    <%= link_to "Home", root_path %>
  </li>

  <% if user_signed_in? %>
    <li>
    <%= link_to('Logout', destroy_user_session_path, :method => :delete) %>
    </li>
  <% else %>
    <li>
    <%= link_to('Login', login_path)  %>
    </li>
    <li>
      <%= link_to "Sign Up", signup_path %>
    </li>
  <% end %>


</header>
